<?xml version="1.0" encoding="utf-8"?>

<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true" >
        <shape android:shape="rectangle"  >
            <stroke android:width="1dip" android:color="#ff5794aa" />
            <solid android:color="#ff5794aa"></solid>

        </shape>
    </item>
    <item android:state_focused="true">
        <shape android:shape="rectangle"  >
            <stroke android:width="1dip" android:color="#ff24a4cb" />
            <solid  android:color="#ff71b5cb"/>
        </shape>
    </item>
    <item >
        <shape android:shape="rectangle"  >
            <stroke android:width="1dip" android:color="#ff1c6284" />
            <solid android:color="#ff1c6284"></solid>
        </shape>
    </item>
</selector>